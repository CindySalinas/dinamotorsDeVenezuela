body
  font-size 16px
  width 100%
  app-bg(page-bg, font-black)
  font-family 'Open Sans', sans-serif
  background-color #FFF

.container
  display flex
  flex 1 1 auto
  justify-content flex-start
  flex-col-wrap()

.row
  flex-row()
  flex 1

.col
  flex-col()
  flex 1

.p-sm
  padding 10px

.m-t-lg
  margin-top 25px
